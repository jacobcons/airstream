block vars
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    title #{title}
    link(rel="stylesheet" href="css/bundle.css")
  body(class=bodyClasses)
    block facebook
    .wrapper
      block header
          nav.nav.clearfix
            #js-hamburber-menu.hamburger-menu
              span.hamburger-menu__line
            ul.primary-nav
              li
                a.nav__link.primary-nav__link(href="index.html") Home
              li.nav__item--has-sub
                a.nav__link.nav__link--has-sub.primary-nav__link(href="javascript:void(0)") Airstream Photo Booth
                ul.sub-nav
                  li
                    a.nav__link.sub-nav__link(href="interior.html") Interior
                  li
                    a.nav__link.sub-nav__link(href="photo-booth.html") Photo Booth
                  li
                    a.nav__link.sub-nav__link(href="exterior.html") Exterior
                  li
                    a.nav__link.sub-nav__link(href="guest-books.html") Guest Books
                  li
                    a.nav__link.sub-nav__link(href="probs-&-features.html") Props & Features
                  li
                    a.nav__link.sub-nav__link(href="events.html") Events
              li
                a(href="index.html")
                  img.primary-nav__logo(src="images/airstream_studio_logo.png")
              li
                a.nav__link.primary-nav__link(href="prices.html") Prices
              li
                a.nav__link.primary-nav__link(href="social-media.html") Social Media
              li
                a.nav__link.primary-nav__link(href="contact.html") Contact

      block content

      block footer
        footer.footer
          .row.text-center
            .medium-4.small-12
              nav.nav
                ul.list-nav
                  li
                    a.nav__link.list-nav__link(href="index.html") Home
                  li.nav__item--has-sub
                    a.nav__link.nav__link--has-sub.list-nav__link(href="javascript:void(0)") Airstream photo booth
                    ul.sub-nav.sub-nav--static
                      li
                        a.nav__link.sub-nav__link(href="interior.html") Interior
                      li
                        a.nav__link.sub-nav__link(href="photo-booth.html") Photo Booth
                      li
                        a.nav__link.sub-nav__link(href="exterior.html") Exterior
                      li
                        a.nav__link.sub-nav__link(href="guest-books.html") Guest Books
                      li
                        a.nav__link.sub-nav__link(href="props-&-features.html") Props & Features
                      li
                        a.nav__link.sub-nav__link(href="events.html") Events
                  li
                    a.nav__link.list-nav__link(href="prices.html") Prices
                  li
                    a.nav__link.list-nav__link(href="social-media.html") Social Media
            .medium-4.small-12.bottom-spacer
              nav.nav
                ul.list-nav
                  li.list-nav__item
                    a.nav__link.list-nav__link(href="wedding-suppliers.html") Wedding suppliers
                  li.list-nav__item
                    a.nav__link.list-nav__link(href="vintage-shops-sussex.html") Vintage shops sussex
                  li.list-nav__item
                    a.nav__link.list-nav__link(href="vintage-events.html") Vintage events
                  li.list-nav__item
                    a.nav__link.list-nav__link(href="contact.html") Contact
            .medium-4.small-12
              .row.align-center
                a.social-icon(href="https://www.instagram.com/airstream.studiophotobooth/" target="_blank" rel="noopener")
                  include ../includes/icons/instagram.svg
                a.social-icon(href="https://www.facebook.com/airstreamstudios/" target="_blank" rel="noopener")
                  include ../includes/icons/fb.svg
              .row.align-center
                a.social-icon(href="https://www.pinterest.com/airstreamst0274/airstream-studio-vintage-photo-booth-sussex/" target="_blank" rel="noopener")
                  include ../includes/icons/pinterest.svg
                a.social-icon(href="" target="_blank" rel="noopener")
                  include ../includes/icons/flickr.svg
              .row.align-center
                a.social-icon(href="https://twitter.com/airstreamsussex" target="_blank" rel="noopener")
                  include ../includes/icons/twitter.svg
                a.social-icon(href="https://plus.google.com/106091941708335708484" target="_blank" rel="noopener")
                  include ../includes/icons/g+.svg

    block scripts
      script(src="js/main.js")
